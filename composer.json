{
    "name": "curious-inc/file-upload-form-type-bundle",
    "type": "symfony-bundle",
    "description": "Helper bundle for mapping Dropzone.js frontend to Symfony FormType using File entity",
    "authors": [
        {
            "name": "Webber",
            "email": "webber@takken.io"
        }
    ],
    "license": "MIT",
    "require": {
        "doctrine/orm": "~2.4",
        "knplabs/doctrine-behaviors": "~1.1",
        "oneup/uploader-bundle": "~2.0",
        "twig/twig": "~2.1"
    },
    "autoload": {
      "psr-4": {
        "CuriousInc\\FileUploadFormTypeBundle\\": ""
      },
      "exclude-from-classmap": [
        "Tests/"
      ]
    },
    "autoload-dev": {
      "psr-4": { "CuriousInc\\FileUploadFormTypeBundle\\Tests\\": "Tests/" }
    },
    "config": {
      "sort-packages": true
    }
}
